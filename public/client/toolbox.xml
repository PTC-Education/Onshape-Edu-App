<xml xmlns="http://www.w3.org/1999/xhtml" id="toolbox" style="display: none;">

  <sep></sep>
  <category name="Math" colour="#5C68A6">
    <block type="math_number">
      <field name="NUM">0</field>
    </block>
    <block type="math_constants">
      <field name="CONSTANT">PI</field>
    </block>
    <block type="math_singles">
      <field name="OP">ROOT</field>
      <value name="NUM">
        <shadow type="math_number">
          <field name="NUM">9</field>
        </shadow>
      </value>
    </block>
    <block type="math_trigs">
      <field name="OP">SIN</field>
      <value name="NUM">
        <shadow type="math_number">
          <field name="NUM">45</field>
        </shadow>
      </value>
    </block>
    <block type="math_arithmetic">
      <field name="OP">ADD</field>
      <value name="A">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="B">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="math_modulo">
      <value name="DIVIDEND">
        <shadow type="math_number">
          <field name="NUM">64</field>
        </shadow>
      </value>
      <value name="DIVISOR">
        <shadow type="math_number">
          <field name="NUM">10</field>
        </shadow>
      </value>
    </block>
    <block type="math_random_int">
      <value name="FROM">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="TO">
        <shadow type="math_number">
          <field name="NUM">100</field>
        </shadow>
      </value>
    </block>
    <block type="math_random_float"></block>
    <block type="math_rounds">
      <field name="OP">ROUND</field>
      <value name="NUM">
        <shadow type="math_number">
          <field name="NUM">3.1</field>
        </shadow>
      </value>
    </block>
  </category>
  
  <category name="Variables" colour="#A65C81" custom="VARIABLE"></category>

  <category name="Vectors [x,y,z]" colour="#5CA68F">
    <block type="vector2Field">
      <field name="x">0</field>
      <field name="y">0</field>
    </block>
    <block type="vector3Field">
      <field name="x">0</field>
      <field name="y">0</field>
      <field name="z">0</field>
    </block>
    <block type="vector2">
      <value name="x">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="y">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
    </block>
    <block type="vector3">
      <value name="x">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="y">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="z">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
    </block>
    <block type="points"></block>
  </category>

  <sep></sep>

  <category name="Loops" colour="#5CA65C">
    <block type="forLoop">
      <value name="Range">
        <shadow type="vector2Field">
            <field name="x">1</field>
            <field name="y">4</field>
        </shadow>
      </value>
      <value name="Increment">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
  </category>

  <sep></sep>

  <category name="Features" colour="#5ba55b">
    <block type="feature">
      <field name="featurename">FeatureBlocks Feature</field>
      <field name="preconditions"></field>
      <field name="actions"></field>
    </block>
    <block type="preconditions"></block>
    <block type="unionall"></block>
    <block type="colorPart"></block>
  </category>


  <category name="Sketch" colour="FF0004">
    <block type="sketch">
      <field name="skname">Sketch1</field>
      <field name="planename">Top</field>
      <field name="skentities"></field>
    </block>
    <block type='plane'>
      <value name="center_xyz">
        <shadow type="vector3Field">
            <field name="x">0</field>
            <field name="y">0</field>
            <field name="z">5</field>
        </shadow>
      </value>
      <value name="normal_xyz">
        <shadow type="vector3Field">
            <field name="x">0</field>
            <field name="y">0</field>
            <field name="z">1</field>
        </shadow>
      </value>
    </block>
    <sep></sep>
    <sep></sep>
    <block type="skcircle">
      <value name="center_xy">
        <shadow type="vector2Field">
            <field name="x">0</field>
            <field name="y">0</field>
        </shadow>
      </value>
      <value name="radius">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="skrectangle">
      <value name="first_corner_xy">
        <shadow type="vector2Field">
            <field name="x">0</field>
            <field name="y">0</field>
        </shadow>
      </value>
      <value name="second_corner_xy">
        <shadow type="vector2Field">
            <field name="x">1</field>
            <field name="y">1</field>
        </shadow>
      </value>
    </block>
    <block type="skregularpolygon">
      <value name="center_xy">
        <shadow type="vector2Field">
            <field name="x">0</field>
            <field name="y">0</field>
        </shadow>
      </value>
      <value name="vertex_xy">
        <shadow type="vector2Field">
            <field name="x">1</field>
            <field name="y">1</field>
        </shadow>
      </value>
      <value name="sides">
        <shadow type="math_number">
          <field name="NUM">3</field>
        </shadow>
      </value>
    </block>
    <block type="skpoint">
      <value name="position_xy">
        <shadow type="vector2Field">
            <field name="x">1</field>
            <field name="y">1</field>
        </shadow>
      </value>   
    </block>
    <block type="sklinesegment">
      <value name="start_xy">
        <shadow type="vector2Field">
            <field name="x">0</field>
            <field name="y">0</field>
        </shadow>
      </value>
      <value name="end_xy">
        <shadow type="vector2Field">
            <field name="x">1</field>
            <field name="y">1</field>
        </shadow>
      </value>
    </block>
    <block type="skpolyline"></block>
    <block type="sktext">
      <field name="text">Example</field>
    </block>
    <block type="skellipse">
      <value name="center_xy">
        <shadow type="vector2Field">
            <field name="x">0</field>
            <field name="y">0</field>
        </shadow>
      </value>
      <value name="majorRadius">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="minorRadius">
        <shadow type="math_number">
          <field name="NUM">2</field>
        </shadow>
      </value>
    </block>
    <block type="skarc">
      <value name="start_xy">
        <shadow type="vector2Field">
            <field name="x">0</field>
            <field name="y">0</field>
        </shadow>
      </value>
      <value name="mid_xy">
        <shadow type="vector2Field">
            <field name="x">1</field>
            <field name="y">1</field>
        </shadow>
      </value>
      <value name="end_xy">
        <shadow type="vector2Field">
            <field name="x">2</field>
            <field name="y">0</field>
        </shadow>
      </value>
    </block>
    <block type="skellipticalarc">
      <value name="center_xy">
        <shadow type="vector2Field">
            <field name="x">0</field>
            <field name="y">0</field>
        </shadow>
      </value>
      <value name="major_xy">
        <shadow type="vector2Field">
            <field name="x">0</field>
            <field name="y">0</field>
        </shadow>
      </value>
      <value name="majorRadius">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="minorRadius">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="startParameter">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="endParameter">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
  </category>


