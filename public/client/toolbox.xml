  <category name="3D Operations" colour="#745CA6">
    <block type='extrude'>
      <value name="endDepth">
        <shadow type="math_number">
          <field name="NUM">5</field>
        </shadow>
      </value>
    </block>
    <block type='revolve'>
      <value name="degrees">
       <shadow type="math_number">
          <field name="NUM">30</field>
        </shadow>
      </value>
    </block>
    <block type='sweep'></block>
    <block type='loft'></block>
